{
  "name": "react-commentr",
  "version": "1.0.0",
  "description": "React comments component",
  "main": "./dist/index.js",
  "scripts": {
    "test": "npm run test:jest && npm run test:nightwatch",
    "test:nightwatch": "cd ./tests && nightwatch",
    "test:jest": "jest --coverage ./src",
    "lint": "standard ./src/**",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "watch": "NODE_ENV=development webpack-dev-server --config webpack.watch.config.js",
    "preversion": "npm config set tag-version-prefix ''",
    "version": "npm run build && git add -f ./dist/index.js",
    "postversion": "git push --tags && git push",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "files": [
    "dist/**"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nruotsal/react-commentr.git"
  },
  "author": "Niko Ruotsalainen",
  "license": "MIT",
  "devDependencies": {
    "@storybook/react": "4.0.0-alpha.8",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "chromedriver": "2.41.0",
    "clean-webpack-plugin": "0.1.19",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "jest": "23.5.0",
    "nightwatch": "0.9.21",
    "prop-types": "15.6.2",
    "react": "16.5.0",
    "react-dom": "16.5.0",
    "standard": "12.0.1",
    "uglifyjs-webpack-plugin": "1.3.0",
    "url-loader": "1.1.1",
    "webpack": "4.17.2",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.8"
  },
  "peerDependencies": {
    "prop-types": "15.x",
    "react": "16.x",
    "react-dom": "16.x"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "styled-components": "3.4.5"
  }
}
